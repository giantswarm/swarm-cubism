{
  "name": "$name",
  "components": {
    "webserver/flask": {
      "image": "registry.giantswarm.io/$org/cubism",
      "ports": [
        5000
      ],
      "env": [
        "token=$token"
      ],
      "domains": {
        "5000/tcp": [
          "$domain.gigantic.io"
        ]
      },
      "links": [
        {
          "component": "webserver/redis",
          "target_port": 6379
        }
      ],
      "scale": {
        "min": 3
      }
    },
    "webserver/redis": {
      "image": "redis",
      "ports": [
        6379
      ]
    }
  }
}